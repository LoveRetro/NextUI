              "MIMode": "lldb"
            },
            "cwd": "${workspaceFolder}/workspace/all/gametimectl/build/desktop",
            "program": "${workspaceFolder}/workspace/all/gametimectl/build/desktop/gametimectl.elf",
            "preLaunchTask": "make desktop",
            "args": ["start", "foo.txt"],
        },
        {
            "name": "App: Settings",
            "request": "launch",
            "type": "cppdbg",
            "osx": {
              "MIMode": "lldb"
