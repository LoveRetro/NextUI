{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "NextUI Launcher",
            "request": "launch",
            "type": "cppdbg",
            "osx": {
              "MIMode": "lldb"
            },
            "cwd": "${workspaceFolder}/workspace/all/nextui/build/macos",
            "program": "${workspaceFolder}/workspace/all/nextui/build/macos/nextui",
            "preLaunchTask": "C/C++: gcc build macos",
        },
        {
            "name": "App: Battery",
            "request": "launch",
            "type": "cppdbg",
            "osx": {
              "MIMode": "lldb"
            },
            "cwd": "${workspaceFolder}/workspace/all/battery/build/macos",
            "program": "${workspaceFolder}/workspace/all/battery/build/macos/battery",
            "preLaunchTask": "C/C++: gcc build macos - Battery",
        },
        {
            "name": "App: Input",
            "request": "launch",
            "type": "cppdbg",
            "osx": {
              "MIMode": "lldb"
            },
            "cwd": "${workspaceFolder}/workspace/all/minput/build/macos",
            "program": "${workspaceFolder}/workspace/all/minput/build/macos/minput",
            "preLaunchTask": "C/C++: gcc build macos - Minput",
        },
        {
            "name": "App: Gametime",
            "request": "launch",
            "type": "cppdbg",
            "osx": {
              "MIMode": "lldb"
            },
            "cwd": "${workspaceFolder}/workspace/all/gametime/build/macos",
            "program": "${workspaceFolder}/workspace/all/gametime/build/macos/gametime",
            "preLaunchTask": "C/C++: gcc build macos - Gametime",
        },
        {
            "name": "App: Gametimectl",
            "request": "launch",
            "type": "cppdbg",
            "osx": {
              "MIMode": "lldb"
            },
            "cwd": "${workspaceFolder}/workspace/all/gametimectl/build/macos",
            "program": "${workspaceFolder}/workspace/all/gametimectl/build/macos/gametimectl",
            "args": ["start", "foo.txt"],
            "preLaunchTask": "C/C++: gcc build macos - Gametimectl",
        },
        {
            "name": "App: Settings",
            "request": "launch",
            "type": "cppdbg",
            "osx": {
              "MIMode": "lldb"
            },
            "cwd": "${workspaceFolder}/workspace/all/settings/build/macos",
            "program": "${workspaceFolder}/workspace/all/settings/build/macos/settings",
            "preLaunchTask": "C/C++: gcc build macos - Settings",
        }
    ]
}